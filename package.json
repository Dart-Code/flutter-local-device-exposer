{
  	"name": "dartlocaldevice",
	"publisher": "helinx",
	"displayName": "DartLocalDevice",
	"description": "",
	"version": "0.0.4",
	"engines": {
		"vscode": "^1.69.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:dart",
        "onCommand:dartlocaldevice.helloWorld",
		"onCommand:dartlocaldevice.startDaemon"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "dartlocaldevice.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "dartlocaldevice.startDaemon",
				"title": "Start Flutter daemon"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.69.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "16.x",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"eslint": "^8.16.0",
		"glob": "^8.0.3",
		"mocha": "^10.0.0",
		"typescript": "^4.7.2",
		"@vscode/test-electron": "^2.1.3"
	},
	"extensionKind": ["ui"]
}
